# Android fstab file.

/dev/block/mmcblk0p15       /system         ext4    ro,barrier=1,data=ordered wait
/dev/block/mmcblk0p17       /cache          ext4    noatime,nosuid,nodev,barrier=1,data=ordered wait
/dev/block/mmcblk0p18       /data           ext4    noatime,nosuid,nodev,barrier=1,data=ordered,noauto_da_alloc wait
/dev/block/mmcblk0p7        /persist        ext4    nosuid,nodev,barrier=1,data=ordered,nodelalloc wait
